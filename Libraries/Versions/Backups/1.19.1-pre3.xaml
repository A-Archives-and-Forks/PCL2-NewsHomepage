<local:MyCard Margin="0,0,0,15" Title="1.19.1-pre3" CanSwap="True" IsSwaped="True">
		<!--Header-->
		<StackPanel Margin="30,40,30,20">
			<Border Style="{StaticResource verImgBdr}">
				<Border.Background>
					<ImageBrush ImageSource="https://www.lightbeacon.top/pnh/newimgs/1_19/1.19.1-pre3.png" Stretch="Uniform" />
				</Border.Background>
				<Image Source="https://www.lightbeacon.top/pnh/newimgs/1_19/1.19.1-pre3.png" Opacity="0" />
			</Border>
			<Border Style="{StaticResource verTxtBdr}">
				<TextBlock Style="{StaticResource verTxtBlk}" Text="1.19.1 PRE3" />
			</Border>
			<!--Details-->
<TextBlock Text="更改" Style="{StaticResource h2}" />
<StackPanel Style="{StaticResource ListStyle}">
<Ellipse />
<TextBlock Style="{StaticResource ListItem}" Text="将聊天滚动栏移到聊天栏右侧"/>
</StackPanel>
<StackPanel Style="{StaticResource ListStyle}">
<Ellipse />
<TextBlock Style="{StaticResource ListItem}" Text="系统消息现在显示为灰色指示器"/>
</StackPanel>
<TextBlock Text="技术性更改" Style="{StaticResource h2}" />
<StackPanel Style="{StaticResource ListStyle}">
<Ellipse />
<TextBlock Style="{StaticResource ListItem}" Text="自定义服务器可以通过新的网络包来为日常交流建立属于自己的自动补全选项"/>
</StackPanel>
<StackPanel Style="{StaticResource ListStyle}">
<Ellipse />
<TextBlock Style="{StaticResource ListItem}" Text="在社交界面的玩家列表现在将最近发消息的玩家置于顶部"/>
</StackPanel>
<StackPanel Style="{StaticResource ListStyle}">
<Ellipse />
<TextBlock Style="{StaticResource ListItem}" Text="聊天类型 msg_command 已被拆分成 msg_command_incoming 与 msg_command_outgoing"/>
</StackPanel>
<StackPanel Style="{StaticResource ListStyle}">
<Ellipse />
<TextBlock Style="{StaticResource ListItem}" Text="team_name 聊天类型参数被重命名为 target"/>
</StackPanel>
<StackPanel Style="{StaticResource ListStyle}">
<StackPanel Style="{StaticResource ListStyle}">
<Ellipse />
<TextBlock Style="{StaticResource ListItem}" Text="这种聊天类型参数现在用在 msg_command_outgoing"/>
</StackPanel>
</StackPanel>
<GroupBox Header="阅读完整日志" BorderBrush="{DynamicResource ColorBrush3}">
				<Grid UseLayoutRounding="True" Margin="10,10,10,10">
					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="20*"></ColumnDefinition>
						<ColumnDefinition Width="10*"></ColumnDefinition>
						<ColumnDefinition Width="10*"></ColumnDefinition>
					</Grid.ColumnDefinitions>
					<local:MyButton Style="{StaticResource Read0}" EventData="https://minecraft.fandom.com/zh/wiki/Java版1.19.1-pre3" ColorType="Highlight" />
					<local:MyButton Style="{StaticResource Read1}" EventData="https://www.mcbbs.net/thread-1359631-1-1.html"/>
					<local:MyButton Style="{StaticResource Read2}" EventData="https://www.minecraft.net/zh-hans/article/minecraft-1-19-1-pre-release-3" />
				</Grid>
			</GroupBox><TextBlock Text="这次共修复了6个bug捏~" Style="{StaticResource footnote}" /></StackPanel>	
	</local:MyCard>
