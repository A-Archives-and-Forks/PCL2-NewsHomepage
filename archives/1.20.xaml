          <Image Source="https://www.lightbeacon.top/pnh/newsimgs/1_20/1.20-pre7.jpg" Opacity="0" />
        </Border>
        <Border Style="{StaticResource verTxtBdr}">
          <TextBlock Style="{StaticResource verTxtBlk}" Text="1.20-PRE7" />
        </Border>
        <local:MyHint Margin="0,4,0,8" IsWarn="False" Text="1.20正式版预计6月7日发布" />
        <TextBlock Text="更改" Style="{StaticResource h2}" />
        <StackPanel Style="{StaticResource ListStyle}">
          <Ellipse />
          <TextBlock Style="{StaticResource ListItem}" Text=" 为提升安全性。游戏现在会检测存档文件夹内使用的符号链接" />
          <StackPanel Style="{StaticResource ListStyle}">
            <Ellipse />
            <TextBlock Style="{StaticResource ListItem}" Text=" 如果符号链接的目标不在用户配置的准许列表中，游戏不会载入该存档" />
            <StackPanel Style="{StaticResource ListStyle}">
              <Ellipse />
              <TextBlock Style="{StaticResource ListItem}" Text=" 注：存档文件夹本身仍然可以被链接指向" />
            </StackPanel>
            <Ellipse />
            <TextBlock Style="{StaticResource ListItem}" Text=" 准许的符号链接目标列表储存在 allowed_symlinks.txt 文件中，具体详见完整日志" />
          </StackPanel>
        </StackPanel>
        <GroupBox Header="阅读完整日志" BorderBrush="{DynamicResource ColorBrush3}">
          <Grid UseLayoutRounding="True" Margin="10,10,10,10">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="20*" />
